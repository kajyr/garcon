{
  "name": "kservez",
  "version": "1.0.1",
  "main": "server.ts",
  "author": "Carlo Panzi <carlo@uberresearch.com>",
  "license": "MIT",
  "dependencies": {
    "rc": "^1.2.8"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  },
  "scripts": {
    "start": "bin/kservez",
    "watch": "nodemon"
  },
  "keywords": [
    "http",
    "server",
    "proxy"
  ],
  "bin": {
    "kservez": "bin/kservez"
  },
  "homepage": "https://github.com/kajyr/kservez",
  "nodemonConfig": {
    "watch": [
      "src/*"
    ],
    "ext": "js",
    "ignore": [
      "**/*.test.js"
    ],
    "delay": "500",
    "exec": "bin/kservez public"
  }
}
