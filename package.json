{
  "name": "kservez",
  "version": "1.2.0",
  "main": "bin/kservez",
  "author": "Carlo <carlo.panzi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "mclip": "^0.3.2",
    "node-forge": "^1.2.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.1.0",
    "nodemon": "^2.0.15",
    "release-it": "^14.12.4"
  },
  "scripts": {
    "start": "bin/kservez",
    "watch": "nodemon",
    "release": "release-it"
  },
  "keywords": [
    "http",
    "server",
    "proxy"
  ],
  "bin": {
    "kservez": "bin/kservez"
  },
  "homepage": "https://github.com/kajyr/kservez",
  "nodemonConfig": {
    "watch": [
      "src/*"
    ],
    "ext": "js",
    "ignore": [
      "**/*.test.js"
    ],
    "delay": "500",
    "exec": "bin/kservez public"
  }
}
